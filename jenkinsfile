#!/bin/env groovy
try{
    node (){
        stage('Checkout'){
            try{
                checkout scm
            }catch (error){
                echo "${error}"
            }
        }
        stage('build'){
            echo 'Building the code'
        }
        stage('QA'){
            echo 'Test is complete'
        }
        stage('PRODUCTION'){
            echo 'Product is ready'
        }
    }
}finally {
    echo "Done!"
}
